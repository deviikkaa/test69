public class AddTwoNumbers {
    public static void main(String[] args) {
        // Declare two variables to store numbers
        int num1 = 5;  
        int num2 = 7;  

        int sum = num1 + num2;
        System.out.println("The sum of " + num1 + " and " + num2 + " is " + sum);
    }
}
